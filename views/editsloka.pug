extends layout

block content
  h1 Ramayana Sangraha - Edit Entry
  p.lead #{sloka.sloka_or_gadya}
  hr
  form.sloka-form(onsubmit="return confirm('Are you sure you want to submit?');" method='POST')
    input(type='hidden', name='_csrf', value=_csrf)
